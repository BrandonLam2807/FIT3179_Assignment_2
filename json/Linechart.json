{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/BrandonLam2807/FIT3179_Assignment_2/refs/heads/main/linechart_aus_data.csv"
    },
    "background": "#d6dbdf",
    "vconcat": [
      {
        "width": 500,
        "height": 240,
    
        "mark": "line",
        "encoding": {
          "x": {
            "field": "startDate",
            "type": "temporal",
            "scale": {"domain": {"param": "brush"}},
            "axis": {"title": "Date", "tickCount": 10}
          },
          "y": {"field": "Insured Cost", "type": "quantitative", "axis": {"title": "Insured Cost"}}
        }
      },
      {
        "width": 500,
        "height": 50,
        "mark": "area",
        "params": [
          {"name": "brush", "select": {"type": "interval", "encodings": ["x"]}}
        ],
        "encoding": {
          "x": {"field": "startDate", "type": "temporal", "axis": {"tickCount": 10}, "title": "Date"},
          "y": {
            "field": "Insured Cost",
            "type": "quantitative",
            "axis": {"tickCount": 3, "grid": false}
          }
        }
      }
    ]
  } 